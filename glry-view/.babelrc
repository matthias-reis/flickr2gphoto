{
  "presets": [
    ["@babel/preset-env", false],
    [
      "@babel/preset-react",
      {
        "pragma": "h"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal" }],
    "@babel/plugin-proposal-optional-chaining"
  ]
}
